<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>OnlineAuction</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag 'public/application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'public/application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <%= render 'layouts/jumbotron' %>
      <%= render 'layouts/navigator' %>
      <%= yield %>
      <%= render 'layouts/footer' %>
  </body>
</html>
